app: vscode
-

genie show: user.vscode('chatgpt-vscode.view.focus')
genie new: 
    user.vscode('chatgpt-vscode.view.focus')
    user.vscode('chatgpt-vscode.newChat')

genie add test:
    key(ctrl-k) 
    sleep(50ms)
    key(ctrl-shift-1)
genie find bugs:
    key(ctrl-k) 
    sleep(50ms)
    key(ctrl-shift-2)
genie optimize:
    key(ctrl-k) 
    sleep(50ms)
    key(ctrl-shift-3)
genie explain:
    key(ctrl-k) 
    sleep(50ms)
    key(ctrl-shift-4)
genie comment:
    key(ctrl-k) 
    sleep(50ms)
    key(ctrl-shift-5)
genie complete:
    key(ctrl-k) 
    sleep(50ms)
    key(ctrl-shift-6)
genie please:
    key(ctrl-k) 
    sleep(50ms)
    key(ctrl-shift-7)
genie add dock string:
    key(ctrl-k) 
    sleep(50ms)
    key(ctrl-shift-8)
genie [re] format dock string:
    key(ctrl-k) 
    sleep(50ms)
    key(ctrl-shift-9)
